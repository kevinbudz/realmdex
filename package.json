{
  "name": "realmdex",
  "version": "1.0.0",
  "description": "App",
  "main": "src/main.js",
  "scripts": {
    "start": "electron .",
    "webpack-dev": "webpack serve --mode development",
    "electron-dev": "electron .",
    "dev": "concurrently \"npm run webpack-dev\" \"wait-on http://localhost:3000 && npm run electron-dev\"",
    "build": "webpack --mode production",
    "electron:build": "electron-builder -c.extraMetadata.main=dist/main.js"
  },
  "build": {
    "appId": "com.realmdex.app",
    "productName": "RealmDex",
    "directories": {
      "output": "release/"
    },
    "files": [
      "dist/**/*",
      "src/**/*",
      "package.json"
    ],
    "mac": {
      "target": ["dmg", "zip"]
    },
    "win": {
      "target": ["nsis", "portable"]
    },
    "linux": {
      "target": ["AppImage", "deb"]
    }
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@radix-ui/react-tooltip": "^1.0.0",
    "adm-zip": "^0.5.16",
    "lucide-react": "^0.263.1",
    "node-fetch": "^3.3.2",
    "path-browserify": "^1.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "stream-browserify": "^3.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.22.0",
    "@babel/preset-react": "^7.22.0",
    "@electron/remote": "^2.1.2",
    "@tailwindcss/line-clamp": "^0.4.4",
    "autoprefixer": "^10.4.14",
    "babel-loader": "^9.1.2",
    "concurrently": "^8.0.1",
    "css-loader": "^6.8.1",
    "electron": "^24.3.0",
    "electron-builder": "^24.6.4",
    "electron-packager": "^17.1.2",
    "html-webpack-plugin": "^5.5.1",
    "postcss": "^8.4.23",
    "postcss-loader": "^7.3.0",
    "style-loader": "^3.3.3",
    "tailwindcss": "^3.3.2",
    "wait-on": "^7.0.1",
    "webpack": "^5.84.0",
    "webpack-cli": "^5.1.1",
    "webpack-dev-server": "^4.15.0"
  }
}
